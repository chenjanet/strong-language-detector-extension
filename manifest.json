{
    "name": "Strong Language Detector",
    "version": "1.0",
    "description": "Censors strong language on webpages",
    "permissions": [ 
        "storage", 
        "tabs", 
        "activeTab", 
        "scripting"
    ],
    "host_permissions": [
        "http://*/", 
        "https://*/"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup/popup.html"
    },
    "manifest_version": 3
}